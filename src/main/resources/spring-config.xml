<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

        <bean id="contactController" class="coderuz.controller.ContactController">
            <property name="contactService" ref="contactService"/>
            <property name="scannerService" ref="scannerService"/>
        </bean>

        <bean id="contactService" class="coderuz.service.ContactService"/>

        <bean id="scannerService" class="coderuz.service.ScannerService"/>
</beans>